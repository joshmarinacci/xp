on start do {
    mode_running = false
}
on button_clicked do {
    mode_running = not mode_running
    print("mode running",mode_running)
}
on forever do {
    if mode_running {
        print("pressing E")
        keyboard_press('E')
        set_led(RED)

        wait(0.3)
        set_led(BLACK)
        wait(0.3)
        set_led(RED)
        wait(0.3)
        set_led(BLACK)
        wait(0.3)

        keyboard_releaseAll()
        print("waiting")
        wait(60)
    }
}
