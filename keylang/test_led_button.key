{
mode_running = 0
my_button = 0

fun setup() {
    mode_running = false
    my_button = Button(board.SWITCH)
    print("did start")
}
fun my_button_clicked() {
    if(mode_running == true) {
        mode_running = false
    } else {
        mode_running = true
    }
    print("mode running",mode_running)
}

fun loop() {
    if(mode_running) {
        print("pressing E")
        //keyboard_press("E")
        led.color(RED)

        wait(0.3)
        led.color(BLACK)
        wait(0.3)
        led.color(RED)
        wait(0.3)
        led.color(BLACK)
        wait(0.3)

        //keyboard_release_all()
        print("waiting")
        wait(15)
    }
}

}
