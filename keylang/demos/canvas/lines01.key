@board("canvas")

let line
screen = new KCanvas(0,0,1024,768)

@type('start',setup)
fun setup() {

    line = range(20, screen.width-20, 10).map((x) => {[x,20]})
    range(800).every(() => {
        screen.drawPolyLine(line,BLACK)
        line = line.map((pt) => {
            pt + [randf(-1,1), randf(-0.5,0.5)+0.5]
        })
    })
}
