@board('canvas')
let line
let rect
let rects.hue
line = [[10,100], [100,100]] 
let palette = List(BLACK, RED, GREEN, BLUE)
let Line2 = Rect(w:100, h:100, x:50, y:50)
@type("start",setup)
fun setup() {
let colors = choose(palette)
screen = new KCanvas(0,0,500,500)
screen.drawPolyLine(line,colors)
screen.strokeRect(Line2,colors)
let color2 
screen.fillRect(Line2,color2)
}

